<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.metaswitch.com/nsrs/soap/sh/servicedata"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:d="http://www.metaswitch.com/nsrs/soap/sh/documentation"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.metaswitch.com/nsrs/soap/sh/documentation Documentation.xsd"
           targetNamespace="http://www.metaswitch.com/nsrs/soap/sh/servicedata"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           version="3.9">
  <xs:annotation>
    <xs:documentation xml:lang="en">This is the schema for conference companies
       and users on the N-Series system.
    </xs:documentation>
  </xs:annotation>

  <xs:complexType name="tServiceData">
    <xs:choice>
      <xs:element name="NseriesData" type="tNseriesData"/>
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="tNseriesData">
    <xs:choice>

      <xs:element name="Nsrs_Server_BaseInformation" type="tNsrs_Server_BaseInformation">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_SystemLogList" type="tNsrs_Server_SystemLogList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Conference System Log List
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_RecordingReportList" type="tNsrs_Server_RecordingReportList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Conference Recording Report List
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_ConfCompany_BaseInformation" type="tNsrs_Company_BaseInformation">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Company Name: Id
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_ConfUser_BaseInformation" type="tNsrs_User_BaseInformation">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>User Name: Id
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_ConfClassOfService_BaseInformation" type="tNsrs_ConfClassOfService_BaseInformation">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Class of Service Description
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_ConfCompanyList" type="tNsrs_Server_ConfCompanyList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_ConfUserList" type="tNsrs_Server_ConfUserList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_ConfCompany_ConfUserList" type="tNsrs_ConfCompany_ConfUserList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Company Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_ConfCoSList" type="tNsrs_Server_ConfCoSList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_MOHGMGList" type="tNsrs_Server_MOHGMGList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_MOHGlobalMediaGroup_ResourceList" type="tNsrs_MOHGlobalMediaGroup_ResourceList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
              <d:Identifier>Music On Hold - Global Media Group
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Global_ServerList" type="tNsrs_Global_ServerList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Global_ConfUserSearch" type="tNsrs_Global_ConfUserSearch">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Global_ConfCompanySearch" type="tNsrs_Global_ConfCompanySearch">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Global_ConfCoSSearch" type="tNsrs_Global_ConfCoSSearch">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_AvailableBillingRecordsList" type="tNsrs_Server_AvailableBillingRecordsList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Nsrs_Server_RecentlyDownloadedBillingRecordsList" type="tNsrs_Server_RecentlyDownloadedBillingRecordsList">
        <xs:annotation>
          <xs:documentation>
            <d:Identifiers>
              <d:Identifier>Server Name
              </d:Identifier>
            </d:Identifiers>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:choice>
  </xs:complexType>

  <!--**********************************************************************-->
  <!-- Defining useful common types upfront.                                -->
  <!--**********************************************************************-->
  <xs:simpleType name="tTrueFalse">
    <xs:restriction base="xs:string">
      <xs:enumeration value="True"/>
      <xs:enumeration value="False"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tLanguage">
    <xs:restriction base="xs:string">
      <xs:enumeration value="English - American" />
      <xs:enumeration value="English - Canadian" />
      <xs:enumeration value="French - Canadian" />
      <xs:enumeration value="English - United Kingdom" />
      <xs:enumeration value="Spanish - Latin America" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tParticipantCode">
    <xs:restriction base="xs:string">
      <xs:minLength value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tModeratorCode">
    <xs:restriction base="xs:string">
      <xs:minLength value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tClassOfService">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tName">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tMoH">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tInfo">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="2048" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tPhoneNumber">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{0,64}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tEmail">
    <xs:restriction base="xs:string">
      <xs:pattern value="[^@]+@[^\.]+\..+" />
      <xs:minLength value="0" />
      <xs:maxLength value="254" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tAcctNumType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="" />
      <xs:enumeration value="Client/Matter Number" />
      <xs:enumeration value="Account Number" />
      <xs:enumeration value="Billing Number" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tRecordingNotification">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Always" />
      <xs:enumeration value="Optional" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tMaxParticipants">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="150" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tIntegers">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="999" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tDeleteRecordingsKeep">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="30" />
      <xs:maxInclusive value="730" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tDeleteRecordingsWarn">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="60" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="tEnabledDisabled">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Enabled" />
      <xs:enumeration value="Disabled" />
    </xs:restriction>
  </xs:simpleType>

  <!--**********************************************************************-->
  <!-- Types to represent lists of companies and users.                     -->
  <!--**********************************************************************-->
  <xs:complexType name="tNsrs_Server_ConfCompanyList">
    <xs:sequence>
      <xs:element name="Company" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CompanyName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Company Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tNsrs_Server_ConfUserList">
    <xs:sequence>
      <xs:element name="User" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CompanyName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Company Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="UserName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>User Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="ParticipantCode" type="tParticipantCode">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Participant Code</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="ModeratorCode" type="tModeratorCode">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Moderator Code</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tNsrs_ConfCompany_ConfUserList">
    <xs:sequence>
      <xs:element name="User" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="UserName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>User Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="ParticipantCode" type="tParticipantCode">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Participant Code</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="ModeratorCode" type="tModeratorCode">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Moderator Code</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tNsrs_Server_ConfCoSList">
    <xs:sequence>
      <xs:element name="ClassOfService" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ClassOfServiceName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Class Of Service Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tNsrs_Server_MOHGMGList">
    <xs:sequence>
      <xs:element name="GlobalMediaGroup" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="GroupName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Group Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tNsrs_MOHGlobalMediaGroup_ResourceList">
    <xs:sequence>
      <xs:element name="Resource" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ResourceName" type="tName">
              <xs:annotation>
                <xs:documentation>
                  <d:DisplayName>Resource Name</d:DisplayName>
                  <d:Type>String</d:Type>
                  <d:BaseAccess>R-R-R-</d:BaseAccess>
                  <d:Mandatory>No</d:Mandatory>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!--**********************************************************************-->
  <!-- A type to represent enabling of a company.                           -->
  <!--**********************************************************************-->
  <xs:complexType name="tNsrs_Company_BaseInformation">
    <xs:annotation>
      <xs:documentation>
        <d:Actions>
          <d:Action>apply</d:Action>
          <d:Action>delete</d:Action>
          <d:Action>enable</d:Action>
          <d:Action>disable</d:Action>
        </d:Actions>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="ServerName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Server Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWR-R-</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="CompanyName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Company Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="CompanyInfo" type="tInfo">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Company Info</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ContactName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Contact Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ContactPhone" type="tPhoneNumber">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Contact Phone</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="EmailAddress" type="tEmail">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Email Address</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="DialInNumber" type="tPhoneNumber">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Dial In Number</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="UserAccounts" type="tTrueFalse">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>User Accounts</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ActualStatus" type="tEnabledDisabled">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Status</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>R-R-R-</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

    <xs:attribute name="Action">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="apply" />
          <xs:enumeration value="delete" />
          <xs:enumeration value="enable" />
          <xs:enumeration value="disable" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>

  </xs:complexType>

  <!--**********************************************************************-->
  <!-- A type to represent enabling of a user.                              -->
  <!--**********************************************************************-->
  <xs:complexType name="tNsrs_User_BaseInformation">
    <xs:annotation>
      <xs:documentation>
        <d:Actions>
          <d:Action>apply</d:Action>
          <d:Action>delete</d:Action>
          <d:Action>enable</d:Action>
          <d:Action>disable</d:Action>
          <d:Action>generatenewmoderatorcode</d:Action>
          <d:Action>generatenewparticipantcode</d:Action>
        </d:Actions>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="ServerName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Server Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWR-R-</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="CompanyName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Company Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWR-R-</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="UserName" type="tName">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>User Name</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Language" type="tLanguage">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Language</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>Yes</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ParticipantCode" type="tParticipantCode">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Participant Code</d:DisplayName>
            <d:Type>String</d:Type>
            <d:BaseAccess>RWRWRW</d:BaseAccess>
            <d:Mandatory>No</d:Mandatory>
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ModeratorCode" type="tModeratorCode">
        <xs:annotation>
          <xs:documentation>
            <d:DisplayName>Moderator Code</d:DisplayName>
